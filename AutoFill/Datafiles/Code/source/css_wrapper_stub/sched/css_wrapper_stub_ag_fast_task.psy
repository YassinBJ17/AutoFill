/* Implements REQ_FADEX_OS_SRD-SCHED-0010 */
/* Implements REQ_FADEX_OS_SRD-SCHED-0040 */
/* Fast task agent to schedule applicative software */
agent css_wrapper_stub_ag_fast_task(uses SOURCE,
                                  starttime 2 with clock_10ms)
{
   body start
   {
      /* TODO: init */

      /* 2ms for the fast task, then advance to the next tick */
      timebudget budget_css_wrapper_stub_ag_fast_task_compil_3, advance 4 with clock_1ms;
      timebudget budget_css_wrapper_stub_ag_fast_task_1, advance 2 with clock_1ms;
      timebudget budget_css_wrapper_stub_ag_fast_task_compil_1, advance 4 with clock_1ms;

      /* Go to body run */
      next run;
   }

   body run
   {
      /* TODO: runtime */

      /* 2ms for the fast task, then advance to the next tick */
      timebudget budget_css_wrapper_stub_ag_fast_task_compil_4, advance 4 with clock_1ms;
      timebudget budget_css_wrapper_stub_ag_fast_task_2, advance 2 with clock_1ms;
      timebudget budget_css_wrapper_stub_ag_fast_task_compil_2, advance 4 with clock_1ms;

   }
}

