<PHOENIX_CONFIG>
    <DEVICES>
     <!-- Dataloader -->
       <DEVICE PhysicalName="DATALOADER1" ModuleDriver="ARINC615A.DLA.cDeviceArinc615aDLA, ARINC615A.DLA" Wrapper="ARINC615A.Wrapper.cWrapperArinc615aDeviceMgt, ARINC615A.Wrapper">
           <TRANSPORT_LAYER>
               <PARAM Name="LocalIP" Value="10.0.0.99" 	Description="Adresse IP Local" />
               <PARAM Name="LocalPort" Value="3004" 		Description="Port de communication Local" />
            <PARAM Name="NbRetry" Value="3" 			Description="Nombre d'essai" />
             <PARAM Name="BlockSize" Value="8188" 		Description="Taille de block en Octets" />
              <PARAM Name="Timeout_s" Value="2" 			Description="Time out en seconde" />
              <PARAM Name="MaxWaitFor_s" Value="30" 		Description="Temps attente max du calculateur" />
         <PARAM Name="A615Version" Value="3" 		Description="Version ARINC615A (2 = A3, 3 = A4)" />
           </TRANSPORT_LAYER>
<HAL Name="TARGET">
               <PARAM Name="TargetName" Value="SFI_EECFADEX_N" 			Description="Nom de la cible" />
                <PARAM Name="TargetIP" Value="10.0.0.1" 				Description="Adresse IP de la cible" />
             <PARAM Name="TargetPort" Value="59" 						Description="Port de communication de la cible" />
                <PARAM Name="TempDirectory" Value="C:\TEMP\DATALOADER\LOG" 	Description="Dossier pour les fichiers de LOG" />
         </HAL>
    </DEVICE>
 </DEVICES>
    <!-- Master Name -->
  <MASTERS>
     </MASTERS>
</PHOENIX_CONFIG>